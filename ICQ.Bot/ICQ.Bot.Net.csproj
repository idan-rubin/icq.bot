<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>HTTP-based interface created for developers who wish to build ICQ bots.</Description>
    <Copyright>Copyright © Idan Rubin 2020</Copyright>
    <Version>1.4.9</Version>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/idan-rubin/icq.bot</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>icq;bot;chatbot;bot-api;icq-bot</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>ICQ.Bot</PackageId>
    <Authors>Idan Rubin</Authors>
    <Product>ICQ.Bot</Product>
    <PackageReleaseNotes>* Fix file receive mechanism bug</PackageReleaseNotes>
    <AssemblyVersion>1.4.9.0</AssemblyVersion>
    <FileVersion>1.4.9.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
